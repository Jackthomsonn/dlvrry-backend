{
  "name": "functions",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "build": "tsc",
    "serve": "concurrently \"npm run build && firebase emulators:start --import=./seed --export-on-exit=./seed\" \"tsc --watch\"",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase functions:config:set dlvrry.return_url='https://auth.expo.io/@jackthomson/dlvrry' dlvrry.functions_url='dlvrry-33018/us-central1 && firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@dlvrry/dlvrry-common": "git+https://github.com/Jackthomsonn/dlvrry-common.git",
    "@types/cors": "^2.8.9",
    "axios": "^0.21.0",
    "calculate-percentages": "^1.0.0",
    "cors": "^2.8.5",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "stripe": "^8.126.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
