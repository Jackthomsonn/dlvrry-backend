{
  "name": "functions",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "build": "tsc",
    "test": "mocha --reporter spec",
    "serve": "concurrently \"npm run build && firebase emulators:start --import=./seed --export-on-exit=./seed\" \"tsc --watch\"",
    "func": "firebase serve --only functions -o 0.0.0.0",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions firestore:rules",
    "logs": "firebase functions:log",
    "config:get": "firebase functions:config:get > .runtimeconfig.json"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "dlvrry-common": "0.0.28",
    "expo-server-sdk": "^3.6.0",
    "firebase-admin": "^9.11.0",
    "firebase-functions": "^3.14.1",
    "firebase-functions-rate-limiter": "^3.8.3",
    "moment": "^2.29.1",
    "stripe": "^8.165.0",
    "twilio": "^3.66.0"
  },
  "devDependencies": {
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.3.0",
    "mocha": "^9.0.3",
    "request": "^2.88.2",
    "typescript": "^4.3.5"
  },
  "private": true
}
